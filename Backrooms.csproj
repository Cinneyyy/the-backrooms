<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <PublishAot>false</PublishAot>
    <InvariantGlobalization>true</InvariantGlobalization>
    <UseWindowsForms>true</UseWindowsForms>
    <_SuppressWinFormsTrimError>true</_SuppressWinFormsTrimError>
    <AssemblyName>backrooms-game</AssemblyName>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <RootNamespace></RootNamespace>
    <ErrorReport>prompt</ErrorReport>
    <IncludeSymbols>false</IncludeSymbols>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <Optimize>false</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Audio\scholz_speech_1.mp3" />
    <None Remove="Resources\oli_appicon.ico" />
    <None Remove="Resources\Textures\carpet.jpg" />
    <None Remove="Resources\Textures\ceiling.png" />
    <None Remove="Resources\Textures\floor.png" />
    <None Remove="Resources\Textures\oli.png" />
    <None Remove="Resources\Textures\pillar.png" />
    <None Remove="Resources\Textures\Skins\entity.png" />
    <None Remove="Resources\Textures\Skins\freddy_fazbear.png" />
    <None Remove="Resources\Textures\Skins\hazmat_suit.png" />
    <None Remove="Resources\Textures\Skins\huggy_wuggy.png" />
    <None Remove="Resources\Textures\Skins\purple_guy.png" />
    <None Remove="Resources\Textures\square.png" />
    <None Remove="Resources\wall.png" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\Audio\scholz_speech_1.mp3" />
    <EmbeddedResource Include="Resources\oli_appicon.ico">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Resources\Textures\carpet.jpg" />
    <EmbeddedResource Include="Resources\Textures\ceiling.png" />
    <EmbeddedResource Include="Resources\Textures\floor.png" />
    <EmbeddedResource Include="Resources\Textures\oli.png" />
    <EmbeddedResource Include="Resources\Textures\pillar.png" />
    <EmbeddedResource Include="Resources\Textures\Skins\entity.png" />
    <EmbeddedResource Include="Resources\Textures\Skins\freddy_fazbear.png" />
    <EmbeddedResource Include="Resources\Textures\Skins\hazmat_suit.png" />
    <EmbeddedResource Include="Resources\Textures\Skins\huggy_wuggy.png" />
    <EmbeddedResource Include="Resources\Textures\Skins\purple_guy.png" />
    <EmbeddedResource Include="Resources\Textures\square.png" />
    <EmbeddedResource Include="Resources\Textures\wall.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="NAudio" Version="2.2.1" />
  </ItemGroup>

</Project>
